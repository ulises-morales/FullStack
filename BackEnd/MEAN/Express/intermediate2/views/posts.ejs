<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Posts</title>
  </head>
  <body>
    <h1>Posts Page</h1>
    <% for (var i = 0; i < posts.length; i++){ %>
      <li><%= posts[i].title %> - <%= posts[i].author %></li>
    <% } %>
    <br>
    <% posts.forEach(function(post){ %>
      <li><%= post.title %> - <%= post.author %></li>
    <% }); %>
  </body>
</html>
